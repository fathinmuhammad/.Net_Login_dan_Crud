    <!-- BEGIN PAGE LEVEL PLUGINS -->
    <script src="~/assets/global/plugins/bootstrap-datepicker/js/bootstrap-datepicker.min.js" type="text/javascript"></script>
    <script src="~/assets/global/plugins/moment.min.js" type="text/javascript"></script>
    <script src="~/assets/global/plugins/bootstrap-daterangepicker/daterangepicker.min.js" type="text/javascript"></script>
    <!-- END PAGE LEVEL PLUGINS -->
    <!-- BEGIN THEME GLOBAL SCRIPTS -->
    <script src="~/assets/global/scripts/app.js" type="text/javascript"></script>
    <script src="~/assets/modules/js/SaveFrm.js" type="text/javascript"></script>
    <!-- END THEME GLOBAL SCRIPTS -->
    <!-- BEGIN PAGE LEVEL SCRIPTS -->
    <script src="~/assets/pages/scripts/components-date-time-pickers-default.js" type="text/javascript"></script>
    @foreach (String script in (List<String>)TempData["scripts_controller"])
    {
        <script src="~/@(Html.Raw(Url.Content(script)))" type="text/javascript"></script>
    }
    <!-- END PAGE LEVEL SCRIPTS -->
    <!-- BEGIN THEME LAYOUT SCRIPTS -->
    <script src="~/assets/layouts/layout4/scripts/layout.min.js" type="text/javascript"></script>
